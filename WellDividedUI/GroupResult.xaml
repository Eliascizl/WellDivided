<UserControl x:Class="WellDividedUI.GroupResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WellDividedUI"
             mc:Ignorable="d" Height="260" Width="200" HorizontalAlignment="Left" VerticalAlignment="Top">
    <Grid>
        <ListBox x:Name="elementsListBox" Margin="10,60,10,10"/>
        <Label x:Name="overviewLabel" Content="Group ID:\nNumber of elements:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
